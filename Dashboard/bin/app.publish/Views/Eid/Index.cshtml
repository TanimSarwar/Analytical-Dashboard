	//Line Chart Starts

	function CatList() {

		$.ajax({
			url: '/Chart/OLDCAT',
			type: 'POST',
			contentType: "application/json;charset=utf-8",
			dataType: "json",
			success: function (response) {
				if (response.length > 0) {


					console.log(response);
					$('#CatList').html('');

					var result = '';

					result += "<option value='0'>SELECT</option>";
					for (var i = 0; response.length > i; i++) {

						
						

						result += "<option id='" + response[i].Cid + "' value='" + response[i].Cid + "'>" + response[i].Description + "</option>";

						
					}

				}
				$('#CatList').append(result);

			}
		});
	}

	function TotalSales() {

		$.ajax({
			url: '/Map/TotalSales',
			type: 'POST',
			contentType: "application/json;charset=utf-8",
			dataType: "json",
			success: function (response) {
				for (var i = 0; response.length > i; i++) {

					$('#ramadan').html('Ramadan Day: ' + response[i].RamadanDay+'<br>');
		
					
					$('#2019sales').html('Sold value of 2019: <br>' + response[i].RetailACH2019Total.toLocaleString('en-IN'));
					$('#2022sales').html('Sales value of 2022: <br>' + response[i].RetailACH2022Total.toLocaleString('en-IN'));
					$('#2022target').html('Target value of 2022: <br>' + response[i].RetailTarget2022Total.toLocaleString('en-IN'));
				}

				console.log(response.RamadanDay);


				}
				
			
		});
	}

	function SalesData() {

		//var sDate = $('#sdate').val();
		//var tDate = $('#tdate').val();


