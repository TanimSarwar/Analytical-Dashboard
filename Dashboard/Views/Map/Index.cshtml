						valueAxis.title.text = "Quantity";
						series.dataFields.categoryX = "TIME";
						series.tooltipText = "{categoryX}: {Val} TK"
						series.strokeWidth = 2;
						series.minBulletDistance = 15;






						chart.legend = new am4charts.Legend();
						series.name = "Quantity";

